config dhcp 'ge00'
	option interface 'ge00'
	option ignore '1'

config dhcp 'gw01'
	option interface 'gw01'
	option ignore '1'

config dhcp 'gw11'
	option interface 'gw11'
	option ignore '1'

config domain
	option name 'cerowrt.home.lan'
	option ip '172.30.42.1'

config domain
	option name 'gw.home.lan'
	option ip '172.30.42.1'

config domain
	option name 'wpad.home.lan'
	option ip '172.30.42.1'

