config dnsmasq
	option domainneeded	1
	option boguspriv	1
	option filterwin2k	0  # enable for dial on demand
	option localise_queries	1
	option rebind_protection 1  # disable if upstream must serve RFC1918 addresses
	option rebind_localhost 1  # enable for RBL checking and similar services
	#list rebind_domain example.lan  # whitelist RFC1918 responses for domains
	option local	'/home.lan/'
	option domain	'home.lan'
	option expandhosts	1
	option port 0
	option nonegcache	0
	option authoritative	1
	option readethers	1
	option leasefile	'/tmp/dhcp.leases'
	option resolvfile	'/tmp/resolv.conf.auto'
	option cachesize 	150
	option dhcpscript 	'/etc/dnsmasq_net6.sh'
	#list server		'/mycompany.local/1.2.3.4'
	#option nonwildcard	1
	#list interface		br-lan
	#list notinterface	lo
	#list bogusnxdomain     '64.94.110.11'

config 'dhcp' 'se00'
	option interface	'se00'
	option start 	2
	option limit	26
	option leasetime	24h
	option domain	'home.lan'
	
config 'dhcp' 'sw00'
	option interface	'sw00'
	option start 	2
	option limit	26
        list 'dhcp_option' '42,0.0.0.0'    
	option leasetime	24h
	option domain	'home.lan'
	
config 'dhcp' 'sw10'
	option interface	'sw10'
	option start 	2
	option limit	26
        list 'dhcp_option' '42,0.0.0.0'    
	option leasetime	'24h'
	option domain	'home.lan'
	
config 'dhcp' 'gw00'
	option interface	'gw00'
	option start 	2
	option limit	26
	option leasetime	24h
        list 'dhcp_option' '42,0.0.0.0'    
	option domain	'home.lan'

config 'dhcp' 'gw10'
	option interface	'gw10'
	option start 	2
	option limit	26
        list 'dhcp_option' '42,0.0.0.0'    
	option leasetime	24h
	option domain	'home.lan'

config 'dhcp' 'ge00'
	option interface	'ge00'
	option ignore	1
